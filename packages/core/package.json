{
  "name": "@ssml-slate/core",
  "version": "0.0.1",
  "description": "SSML Slate core package",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf dist",
    "lint": "eslint src --ext ts,tsx"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@types/is-hotkey": "^0.1.10",
    "@types/is-url": "^1.2.32",
    "classnames": "^2.3.2",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "slate": "^0.101.4",
    "slate-history": "^0.100.0",
    "slate-react": "^0.101.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1"
  }
} 