{
  "name": "@ssml-slate/core",
  "version": "0.0.1",
  "description": "SSML Slate core package",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "clean": "rimraf dist",
    "lint": "eslint src --ext ts,tsx",
    "install-deps": "npm install"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@types/is-hotkey": "^0.1.10",
    "@types/is-url": "^1.2.32",
    "classnames": "^2.3.2",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "slate": "^0.101.4",
    "slate-history": "^0.100.0",
    "slate-react": "^0.101.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-url": "^8.0.2",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "less": "^4.2.0",
    "postcss": "^8.4.31",
    "postcss-modules": "^6.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
} 